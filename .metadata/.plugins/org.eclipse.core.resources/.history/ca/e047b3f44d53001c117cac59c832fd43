package com.bharath.junit5;

import java.util.Scanner;

public class Pattern {
	public static void main(String[] args)
	{
		Scanner sc=new Scanner(System.in);
		 int n=sc.nextInt();
		 
		 for(int i=1;i<=n;i++)
		 {
			 
			 for(int k=i;k<n-i;k++)
			 {
				 System.out.print(" ");
			 }
			 
			 
			 for(int j=1;j<=i;j++)
			 {
				 System.out.print(j+" ");
			 }
			 System.out.println();
		 }
		
	}

}
